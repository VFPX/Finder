	PROCEDURE chdirtoscope
		Local lcScope
		lcScope	 = Thisform.cScope
		lcScopeType = Thisform.GetScopeType()
		
		Do Case
			Case lcScopeType = 'Project'
				Cd (Justpath(lcScope))
			Case lcScopeType = 'Folder'
				Cd (Justpath(lcScope))
		Endcase
		
		
	ENDPROC

