		For lnI = 1 To Min(This.grdResults.ColumnCount, lnCount)
			loColumn			 = This.grdResults.Columns[lnI]
			loColumn.ColumnOrder = Val(Getwordnum(laColumnInfo[lnI], 1))
			loColumn.Width		 = Val(Getwordnum(laColumnInfo[lnI], 2))
